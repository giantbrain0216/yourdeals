@page "/MobileForgetResetPassword"
@inject ShellNavigationManager navman
<ContentPage Title="Email Confirmation">
    <StackLayout>
        <BlazorWebView VerticalOptions="XF.LayoutOptions.FillAndExpand">
            <NPComplet.YourDeals.Client.Shared.WebUI.Pages.Login.ForgetResetPassword IsMobile="true" OnForgotPassword="showNotification" BackToLogin="gotToLogin" />
        </BlazorWebView>
    </StackLayout>
</ContentPage>

@code {

    async void showNotification(Result<AuthResponseDto> isChanged)
    {
        StateHasChanged();
    }
    async void gotToLogin()
    {
        StateHasChanged();
        await XF.Shell.Current.GoToAsync("..");

    }
}